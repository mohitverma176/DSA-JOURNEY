class Solution {
    public int removeDuplicates(int[] nums) {
    ArrayList<Integer> res = new ArrayList<>();
        if (nums.length == 0) return res.size();

        res.add(nums[0]);   // first element

        for (int i = 1; i < nums.length; i++) {
            if (nums[i] != nums[i - 1]) {
                res.add(nums[i]);
            }
        }
        return res.size();

}}
