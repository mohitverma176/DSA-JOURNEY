class Solution {
    public void rotate(int[] nums, int k) {
        int n=nums.length;
        if(k<0) k=k+n;
        if(k>n) k%=n;
        if(k==0) return;
        reverse(nums,0,n-k-1);
        reverse(nums,n-k,n-1);
        reverse(nums,0,n-1);
    }
    public void reverse(int[] nums,int i,int j){
        if(i>=j){
            return;
        }
        int temp=nums[i];
        nums[i]=nums[j];
        nums[j]=temp;
        reverse(nums,i+1,j-1);
    }
}
